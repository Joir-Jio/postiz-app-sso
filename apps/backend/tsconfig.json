{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "exclude": [
    "**/*.spec.ts",
    "**/*.spec.d.ts",
    "test/**/*",
    "src/**/*.spec.ts",
    "src/services/sso/**/*",
    "src/services/media/**/*"
  ]
}
